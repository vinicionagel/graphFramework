package searchGraphCore;

import graph.Vertex;
/**
 * 
 * @author Roland e Vinicio.
 *
 */
public class Atribute {
	
	private Vertex antecessor;
	private Color cor;
	
	public Vertex getAntecessor() {
		return antecessor;
	}
	public void setAntecessor(Vertex antecessor) {
		this.antecessor = antecessor;
	}
	public Color getCor() {
		return cor;
	}
	public void setCor(Color cor) {
		this.cor = cor;
	}
	
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("Atribute [antecessor=");
		builder.append(antecessor);
		builder.append(", cor=");
		builder.append(cor);
		builder.append("]");
		return builder.toString();
	}
	
}
